{
  "name": "11ty-min-starter",
  "version": "1.0.0",
  "description": "A minimal Eleventy (11ty) SSG starter project featuring SASS support.",
  "main": "",
  "scripts": {
    "start": "npm run sass:watch & npm run 11ty:serve",
    "build": "npm run sass:build & npm run 11ty:build",
    "11ty:serve": "npx @11ty/eleventy --serve",
    "11ty:build": "npx @11ty/eleventy",
    "sass:watch": "node-sass --watch --output-style compressed src/assets/_scss/style.scss src/assets/css/style.min.css",
    "sass:build": "node-sass --output-style compressed src/assets/_scss/style.scss src/assets/css/style.min.css"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "sass",
    "scss",
    "css",
    "html",
    "static site generator",
    "ssg"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "node-sass": "^8.0.0"
  }
}
